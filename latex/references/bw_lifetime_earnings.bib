}

@article{dezhbakhsh_interpolation_2022,
	title = {Interpolation and shock persistence of prewar {U}.{S}. macroeconomic time series: {A} reconsideration},
	volume = {213},
	issn = {0165-1765},
	shorttitle = {Interpolation and shock persistence of prewar {U}.{S}. macroeconomic time series},
	url = {https://www.sciencedirect.com/science/article/pii/S0165176522000623},
	doi = {10.1016/j.econlet.2022.110386},
	abstract = {The U.S. prewar output series exhibit smaller shock-persistence than postwar-series. Some studies suggest this may be due to linear interpolation used to generate missing prewar data. Monte Carlo simulations that support this view generate large standard-errors, making such inference imprecise. We assess analytically the effect of linear interpolation on a nonstationary process. We find that interpolation indeed reduces shock-persistence, but the interpolated series can still exhibit greater shock-persistence than a pure random walk. Moreover, linear interpolation makes the series periodically nonstationary, with parameters of the data generating process and the length of the interpolation time-segments affecting shock-persistence in conflicting ways.},
	urldate = {2024-07-03},
	journal = {Economics Letters},
	author = {Dezhbakhsh, Hashem and Levy, Daniel},
	month = apr,
	year = {2022},
	keywords = {Linear interpolation, Nonstationary series, Periodic nonstationarity, Prewar US time series, Random walk, Shock-persistence, Stationary series},
	pages = {110386},
	file = {ScienceDirect Snapshot:C\:\\Users\\onequ\\Zotero\\storage\\LX8XDVQF\\S0165176522000623.html:text/html;Submitted Version:C\:\\Users\\onequ\\Zotero\\storage\\AUBIR7C4\\Dezhbakhsh and Levy - 2022 - Interpolation and shock persistence of prewar U.S..pdf:application/pdf},
}

